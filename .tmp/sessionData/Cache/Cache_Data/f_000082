    }
  }, [history, activeSession, viewingSessionId]);
  useEffect(() => {
    try {
      localStorage.setItem(FS_ROOTS_KEY, JSON.stringify(fsRoots));
    } catch {
    }
  }, [fsRoots]);
  const abortRunning = () => {
    requestIdRef.current += 1;
    setIsGenerating(false);
  };
  const archiveActiveSessionIfNeeded = () => {
    if (!shouldArchive(activeSession)) return;
    const archived = {
      ...activeSession,
      title: inferTitle(activeSession.messages),
      updatedAt: Date.now()
    };
    setHistory((prev) => [archived, ...prev.filter((s) => s.id !== archived.id)]);
  };
  const updateSessionMessages = (sessionId, updater) => {
    if (sessionId) {
      let updated = false;
      setHistory(
        (prev) => prev.map((s) => {
          if (s.id !== sessionId) return s;
          updated = true;
          const nextMessages = updater(s.messages);
          return {
            ...s,
            messages: nextMessages,
            title: s.title === "New chat" ? inferTitle(nextMessages) : s.title,
            updatedAt: Date.now()
          };
        })
      );
      if (updated) return;
      setViewingSessionId(null);
    }
    setActiveSession((prev) => {
      const nextMessages = updater(prev.messages);
      return {
        ...prev,
        messages: nextMessages,
        title: prev.title === "New chat" ? inferTitle(nextMessages) : prev.title,
        updatedAt: Date.now()
      };
    });
  };
  const startNewChat = () => {
    abortRunning();
    archiveActiveSessionIfNeeded();
    const now = Date.now();
    setActiveSession({
      id: createId(),
      title: "New chat",
      messages: defaultGreeting(),
      createdAt: now,
      updatedAt: now
    });
    setViewingSessionId(null);
    setActiveView("chat");
    setInputValue("");
  };
  const openSessionFromHistory = (sessionId) => {
    abortRunning();
    setViewingSessionId(sessionId);
    setActiveView("chat");
    setInputValue("");
  };
  const openCurrentChat = () => {
    abortRunning();
    setViewingSessionId(null);
    setActiveView("chat");
    setInputValue("");
  };
  const saveLlmConfig = async () => {
    setLlmStatus(null);
    const temperature = Number.isFinite(Number(llmTemperature)) ? Number(llmTemperature) : void 0;
    const maxTokens = Number.isFinite(Number(llmMaxTokens)) ? Number(llmMaxTokens) : void 0;
    const cfg = await window.api.llm.setConfig({
      baseUrl: llmBaseUrl.trim(),
      model: llmModel.trim(),
      temperature,
      maxTokens,
      apiKey: llmApiKey,
      clearApiKey: llmClearApiKey
    });
    setLlmConfig(cfg);
    setLlmApiKey("");
    setLlmClearApiKey(false);
    setLlmStatus("已保存");
  };
  const testLlmConfig = async () => {
    setLlmStatus(null);
    await saveLlmConfig();
    const res = await window.api.llm.chat({
      messages: [{ role: "user", content: "ping" }]
    });
    setLlmStatus(`测试成功：${res.text}`);
  };
  const getFsRoot = () => {
    return fsRoots[viewedSessionId] ?? null;
  };
  const setFsRoot = (root) => {
    setFsRoots((prev) => ({ ...prev, [viewedSessionId]: root }));
  };
  const summarizeEntries = (entries) => {
    const fileCount = entries.filter((e) => e.type === "file").length;
    const dirCount = entries.filter((e) => e.type === "dir").length;
    const extCount = /* @__PURE__ */ new Map();
    for (const e of entries) {
      if (e.type !== "file") continue;
      const m = e.path.toLowerCase().match(/\.([a-z0-9]+)$/);
      const ext = m?.[1] ? `.${m[1]}` : "(noext)";
      extCount.set(ext, (extCount.get(ext) ?? 0) + 1);
    }
    const topExt = [...extCount.entries()].sort((a, b) => b[1] - a[1]).slice(0, 10).map(([k, v]) => `${k}:${v}`).join("  ");
    return { fileCount, dirCount, topExt };
  };
  const buildFsPrompt = (args) => {
    return [
      "你是一个桌面应用的文件整理助手。目标：在不越界的前提下整理/批量修改选中文件夹内的文件。",
      "",
      "输出要求：只能输出 JSON（不要输出任何其它文本；不要输出 <think> 或解释过程）。",
      "",
      "路径要求：",
      "- 所有 path/from/to 都必须是相对根目录的路径（用 / 分隔），不能包含盘符、不能以 / 开头、不能包含 ..",
      "",
      "允许的操作（只能使用这些）：",
      '  - { "op": "mkdir", "path": "..." }',
      '  - { "op": "move", "from": "...", "to": "..." }',
      '  - { "op": "delete", "path": "..." }',
      '  - { "op": "replaceText", "path": "...", "mode": "literal|regex", "search": "...", "replace": "...", "flags": "gim" }',
      "",
      "你必须返回：",
      '{ "summary": "...", "operations": [ ... ] }',
