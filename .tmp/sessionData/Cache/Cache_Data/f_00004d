function extractJson(text) {
  const fenced = text.match(/```json\s*([\s\S]*?)\s*```/i);
  if (fenced?.[1]) return fenced[1];
  const fencedAny = text.match(/```([\s\S]*?)```/i);
  if (fencedAny?.[1]) return fencedAny[1];
  return text;
}
function safeParsePlan(text) {
  const jsonText = extractJson(text).trim();
  const parsed = JSON.parse(jsonText);
  if (!parsed || typeof parsed !== "object") throw new Error("计划不是对象");
  const summary = typeof parsed.summary === "string" ? parsed.summary : "";
  const operations = Array.isArray(parsed.operations) ? parsed.operations : null;
  if (!operations) throw new Error("计划缺少 operations 数组");
  return { summary, operations };
}
function formatBytes(bytes) {
  if (!Number.isFinite(bytes)) return "-";
  const units = ["B", "KB", "MB", "GB"];
  let size = bytes;
  let idx = 0;
  while (size >= 1024 && idx < units.length - 1) {
    size /= 1024;
    idx += 1;
  }
  return `${size.toFixed(idx === 0 ? 0 : 1)} ${units[idx]}`;
}
export function CoworkFilesView(props) {
  _s();
  const [root, setRoot] = useState(null);
  const [entries, setEntries] = useState([]);
  const [instruction, setInstruction] = useState(
    "整理这个文件夹：按扩展名归档到子目录；并把所有 .md 里出现的“TODO”替换成“待办”。"
  );
  const [rawPlanText, setRawPlanText] = useState("");
  const [plan, setPlan] = useState(null);
  const [preview, setPreview] = useState(null);
  const [errors, setErrors] = useState(null);
  const [status, setStatus] = useState(null);
  const [busy, setBusy] = useState(false);
  const fileEntries = useMemo(() => entries.filter((e) => e.type === "file"), [entries]);
  const dirEntries = useMemo(() => entries.filter((e) => e.type === "dir"), [entries]);
  const chooseFolder = async () => {
    setStatus(null);
    setErrors(null);
    const selected = await window.api.fs.selectDirectory();
    if (!selected) return;
    setRoot(selected);
    setPlan(null);
    setRawPlanText("");
    setPreview(null);
    setErrors(null);
    setBusy(true);
    try {
      const list = await window.api.fs.list({ root: selected, maxEntries: 5e3 });
      setEntries(list);
      setStatus(`已载入：${list.length} 项`);
    } catch (e) {
      setStatus(e?.message ? String(e.message) : "读取目录失败");
    } finally {
      setBusy(false);
    }
  };
  const generatePlan = async () => {
    if (!root) {
      setStatus("请先选择文件夹");
      return;
    }
    setBusy(true);
    setStatus(null);
    setErrors(null);
    setPlan(null);
    setPreview(null);
    try {
      const filesForPrompt = fileEntries.slice(0, 300).map((f) => `${f.path} (${formatBytes(f.size)})`).join("\n");
      const prompt = [
        "你是一个桌面应用的文件整理助手。目标：在不越界的前提下整理/批量修改选中文件夹内的文件。",
        "",
        "约束：",
        "- 只能返回 JSON，不能返回其他文本。",
        "- 所有 path/from/to 都必须是相对根目录的路径（用 / 分隔），不能包含盘符或 ..",
        "- 只使用下面的操作：",
        '  - { "op": "mkdir", "path": "..." }',
        '  - { "op": "move", "from": "...", "to": "..." }',
        '  - { "op": "delete", "path": "..." }',
        '  - { "op": "replaceText", "path": "...", "mode": "literal|regex", "search": "...", "replace": "...", "flags": "gim" }',
        "",
        "你必须返回：",
        '{ "summary": "...", "operations": [ ... ] }',
        "",
        "用户需求：",
        instruction,
        "",
        "根目录文件清单（最多 300 个文件）：",
        filesForPrompt,
        ""
      ].join("\n");
      const res = await window.api.llm.chat({ messages: [{ role: "user", content: prompt }] });
      setRawPlanText(res.text);
      const parsed = safeParsePlan(res.text);
      setPlan(parsed);
      const check = await window.api.fs.applyPlan({ root, operations: parsed.operations, commit: false });
      setPreview(check.preview);
      setErrors(check.errors.length ? check.errors : null);
      setStatus(check.ok ? "计划已生成（可预览后执行）" : "计划已生成，但存在错误（请调整后重试）");
    } catch (e) {
      setStatus(e?.message ? String(e.message) : "生成计划失败");
    } finally {
      setBusy(false);
    }
  };
  const applyPlanNow = async () => {
    if (!root || !plan) return;
    setBusy(true);
    setStatus(null);
    setErrors(null);
    try {
      const res = await window.api.fs.applyPlan({ root, operations: plan.operations, commit: true });
      setPreview(res.preview);
      setErrors(res.errors.length ? res.errors : null);
      setStatus(res.ok ? "执行完成" : "执行完成，但有错误");
      const list = await window.api.fs.list({ root, maxEntries: 5e3 });
      setEntries(list);
    } catch (e) {
      setStatus(e?.message ? String(e.message) : "执行失败");
    } finally {
      setBusy(false);
    }
  };
