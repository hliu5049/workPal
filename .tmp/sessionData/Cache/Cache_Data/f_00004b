const STORAGE_KEY = "workpal.chat.v1";
function createId() {
  const cryptoAny = crypto;
  if (cryptoAny?.randomUUID) return cryptoAny.randomUUID();
  return `${Date.now()}-${Math.random().toString(16).slice(2)}`;
}
function defaultGreeting() {
  return [{ role: "ai", content: "Hello! How can I help you today?", createdAt: Date.now() }];
}
function inferTitle(messages) {
  const firstUser = messages.find((m) => m.role === "user" && m.content.trim());
  if (!firstUser) return "New chat";
  const oneLine = firstUser.content.trim().replace(/\s+/g, " ");
  return oneLine.length > 40 ? `${oneLine.slice(0, 40)}…` : oneLine;
}
function shouldArchive(session) {
  const meaningful = session.messages.some((m) => m.role === "user" && m.content.trim());
  return meaningful;
}
function Layout() {
  _s();
  const [isCollapsed, setIsCollapsed] = useState(false);
  const [activeView, setActiveView] = useState("chat");
  const [history, setHistory] = useState([]);
  const [activeSession, setActiveSession] = useState(() => {
    const now = Date.now();
    return {
      id: createId(),
      title: "New chat",
      messages: defaultGreeting(),
      createdAt: now,
      updatedAt: now
    };
  });
  const [viewingSessionId, setViewingSessionId] = useState(null);
  const [inputValue, setInputValue] = useState("");
  const requestIdRef = useRef(0);
  const [isGenerating, setIsGenerating] = useState(false);
  const [llmConfig, setLlmConfig] = useState(null);
  const [llmBaseUrl, setLlmBaseUrl] = useState("");
  const [llmModel, setLlmModel] = useState("");
  const [llmTemperature, setLlmTemperature] = useState("0.7");
  const [llmMaxTokens, setLlmMaxTokens] = useState("1024");
  const [llmApiKey, setLlmApiKey] = useState("");
  const [llmClearApiKey, setLlmClearApiKey] = useState(false);
  const [llmStatus, setLlmStatus] = useState(null);
  const historySorted = useMemo(() => {
    return [...history].sort((a, b) => b.updatedAt - a.updatedAt);
  }, [history]);
  const viewedSession = useMemo(() => {
    if (!viewingSessionId) return activeSession;
    const found = history.find((s) => s.id === viewingSessionId);
    return found ?? activeSession;
  }, [activeSession, history, viewingSessionId]);
  useEffect(() => {
    try {
      const raw = localStorage.getItem(STORAGE_KEY);
      if (!raw) return;
      const parsed = JSON.parse(raw);
      if (parsed.history && Array.isArray(parsed.history)) {
        setHistory(parsed.history);
      }
      if (parsed.active && parsed.active.id) {
        setActiveSession(parsed.active);
      }
      if (typeof parsed.viewingSessionId === "string" || parsed.viewingSessionId === null) {
        setViewingSessionId(parsed.viewingSessionId);
      }
    } catch {
    }
  }, []);
  useEffect(() => {
    void (async () => {
      try {
        const cfg = await window.api.llm.getConfig();
        setLlmConfig(cfg);
        setLlmBaseUrl(cfg.baseUrl ?? "");
        setLlmModel(cfg.model ?? "");
        setLlmTemperature(String(cfg.temperature ?? 0.7));
        setLlmMaxTokens(String(cfg.maxTokens ?? 1024));
      } catch (e) {
        setLlmStatus(e?.message ? String(e.message) : "无法读取 LLM 配置");
      }
    })();
  }, []);
  useEffect(() => {
    try {
      localStorage.setItem(
        STORAGE_KEY,
        JSON.stringify({
          history,
          active: activeSession,
          viewingSessionId
        })
      );
    } catch {
    }
  }, [history, activeSession, viewingSessionId]);
  const abortRunning = () => {
    requestIdRef.current += 1;
    setIsGenerating(false);
  };
  const archiveActiveSessionIfNeeded = () => {
    if (!shouldArchive(activeSession)) return;
    const archived = {
      ...activeSession,
      title: inferTitle(activeSession.messages),
      updatedAt: Date.now()
    };
    setHistory((prev) => [archived, ...prev.filter((s) => s.id !== archived.id)]);
  };
  const startNewChat = () => {
    abortRunning();
    archiveActiveSessionIfNeeded();
    const now = Date.now();
    setActiveSession({
      id: createId(),
      title: "New chat",
      messages: defaultGreeting(),
      createdAt: now,
      updatedAt: now
    });
    setViewingSessionId(null);
    setActiveView("chat");
    setInputValue("");
  };
  const openSessionFromHistory = (sessionId) => {
    abortRunning();
    setViewingSessionId(sessionId);
    setActiveView("chat");
    setInputValue("");
  };
  const openCurrentChat = () => {
    abortRunning();
    setViewingSessionId(null);
    setActiveView("chat");
    setInputValue("");
  };
  const saveLlmConfig = async () => {
    setLlmStatus(null);
    const temperature = Number.isFinite(Number(llmTemperature)) ? Number(llmTemperature) : void 0;
    const maxTokens = Number.isFinite(Number(llmMaxTokens)) ? Number(llmMaxTokens) : void 0;
    const cfg = await window.api.llm.setConfig({
      baseUrl: llmBaseUrl.trim(),
      model: llmModel.trim(),
      temperature,
      maxTokens,
      apiKey: llmApiKey,
      clearApiKey: llmClearApiKey
    });
    setLlmConfig(cfg);
    setLlmApiKey("");
    setLlmClearApiKey(false);
    setLlmStatus("已保存");
  };
  const testLlmConfig = async () => {
    setLlmStatus(null);
    await saveLlmConfig();
    const res = await window.api.llm.chat({
      messages: [{ role: "user", content: "ping" }]
    });
    setLlmStatus(`测试成功：${res.text}`);
  };
  const appendMessageToSession = (sessionId, message) => {
    if (sessionId) {
      let updated = false;
      setHistory(
        (prev) => prev.map((s) => {
          if (s.id !== sessionId) return s;
          updated = true;
          const nextMessages = [...s.messages, message];
          return {
            ...s,
            messages: nextMessages,
            title: s.title === "New chat" ? inferTitle(nextMessages) : s.title,
            updatedAt: Date.now()
          };
        })
      );
      if (updated) return;
      setViewingSessionId(null);
    }
    setActiveSession((prev) => {
      const nextMessages = [...prev.messages, message];
      return {
        ...prev,
        messages: nextMessages,
        title: prev.title === "New chat" ? inferTitle(nextMessages) : prev.title,
        updatedAt: Date.now()
      };
    });
  };
  const handleSendMessage = () => {
    if (!inputValue.trim()) return;
    abortRunning();
    const requestId = requestIdRef.current;
    setIsGenerating(true);
    const question = inputValue.trim();
    const targetSessionId = viewingSessionId;
    const userMsg = { role: "user", content: question, createdAt: Date.now() };
    setInputValue("");
    appendMessageToSession(targetSessionId, userMsg);
    void (async () => {
      try {
        const prior = viewedSession.messages.filter((m) => m.content.trim()).map((m) => ({
          role: m.role === "ai" ? "assistant" : "user",
          content: m.content
        }));
        const res = await window.api.llm.chat({
          messages: [...prior, { role: "user", content: question }]
        });
        if (requestIdRef.current !== requestId) return;
        appendMessageToSession(targetSessionId, {
          role: "ai",
          content: res.text,
          createdAt: Date.now()
        });
      } catch (e) {
        if (requestIdRef.current !== requestId) return;
        appendMessageToSession(targetSessionId, {
          role: "ai",
          content: e?.message ? String(e.message) : "LLM 调用失败",
          createdAt: Date.now()
        });
      } finally {
        if (requestIdRef.current === requestId) {
          setIsGenerating(false);
        }
      }
    })();
  };
  const handleKeyDown = (e) => {
    if (e.key === "Enter" && !e.shiftKey) {
      e.preventDefault();
      handleSendMessage();
    }
  };
